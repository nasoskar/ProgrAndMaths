    "import numpy as np\n",
    "\n",
    "w = 1\n",
    "b1,b2 = 2\n",
    "\n",
    "def sigmoid(X):\n",
    "\n",
    "    z1 = np.dot(w,X) + b1\n",
    "    a1 = (1/1 - np.exp(-z1))\n",
    "\n",
    "    z2 = np.dot(w,a1) + b2\n",
    "    a2 = (1/1 - np.exp(-z2))\n",
    "\n",
    "    return a2\n"
   ]
