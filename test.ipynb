    "import numpy as np\n",
    "\n",
    "def sigmoid(x):\n",
    "\n",
    "    s = 1/(1 + np.exp(-x))\n",
    "\n",
    "    return s\n",
    "\n",
    "def derivative_sigmoid(s):\n",
    "\n",
    "    ds = s*(1-s)\n",
    "\n",
    "    return ds\n",
    "\n",
    "w = [1,2]\n",
    "x = [2,4]\n",
    "b = 3\n",
    "z = np.dot(w,x) + b\n",
    "a = sigmoid(z)\n",
    "da = derivative_sigmoid(a)\n",
    "print(a,da)\n"
   ]
  }
